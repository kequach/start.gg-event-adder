{
  "name": "discord-event-adder",
  "private": true,
  "version": "1.0.0",
  "description": "Discord bot that periodically adds events to a server",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "node app.js --test",
    "test-api": "node app.js --api-only",
    "manual": "node app.js --manual",
    "trigger": "node app.js --trigger",
    "clear": "node app.js --clear",
    "test-data": "node test-api.js",
    "test-melee": "node test-api.js --games=1",
    "test-ultimate": "node test-api.js --games=1386",
    "test-smash": "node test-api.js --games=1,1386",
    "test-fgc": "node test-api.js --games=43868,49783,33945"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.0"
  }
}
