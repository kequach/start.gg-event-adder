# Discord Bot Token (required for Discord functionality)
DISCORD_TOKEN=your_discord_bot_token_here

# Start.gg API Token (required for accessing start.gg API)
# Get yours at: https://start.gg/admin/profile/developer
START_GG_TOKEN=your_start_gg_api_token_here

# Country code for tournament queries (ISO 3166-1 alpha-2 format)
# Examples: DE (Germany), US (United States), JP (Japan), FR (France), etc.
# Default: DE (Germany)
COUNTRY_CODE=DE

# Videogame IDs to filter tournaments (comma-separated)
# Common game IDs:
# 1 = Super Smash Bros. Melee
# 1386 = Super Smash Bros. Ultimate  
# 43868 = Street Fighter 6
# 49783 = Tekken 8
# 33945 = Guilty Gear Strive
# 1095 = Fortnite
# Default: 1 (Melee only)
VIDEOGAME_IDS=1